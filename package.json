{
    "name": "ulss",
    "displayName": "Ulysses Style Sheets",
    "description": "Syntax highlighting and snippets for .ulss (Ulysses Style Sheet) files.",
    "icon": "icon.png",
    "publisher": "Ulysses",
    "version": "1.0.1",
    "repository": "https://github.com/soulmen/ulss-vscode",
    "license": "MIT",
    "engines": {
        "vscode": "^1.49.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "ulss",
            "aliases": ["ULSS", "ulss"],
            "extensions": [".ulss"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "ulss",
            "scopeName": "source.ulss",
            "path": "./syntaxes/ulss.json"
        }],
        "snippets": [{
            "language": "ulss",
            "path": "./snippets/snippets.json"
        }]
    }
}